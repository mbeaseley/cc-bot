{
  "name": "cc-bot",
  "version": "1.0.0",
  "description": "A general discord bot with multiple functionalities",
  "main": "dist/index.js",
  "scripts": {
    "tsc": "tsc && copyfiles -u 1 ./src/assets/images/*.png build",
    "postinstall": "npm run tsc",
    "prestart": "npm run tsc",
    "start": "node ./build/main.js",
    "start:dev": "TS_NODE_DEV=development ts-node-dev src/main.ts",
    "test": "jest --detectOpenHandles",
    "test:watch": "jest --watch --detectOpenHandles",
    "npm:clear": "rm -rf $PWD/node_modules && rm $PWD/package-lock.json && npm cache verify && npm install"
  },
  "keywords": [],
  "author": "",
  "license": "ISC",
  "repository": {},
  "dependencies": {
    "@typeit/discord": "^4.0.9",
    "@types/node": "^15.3.0",
    "@types/underscore": "^1.11.0",
    "@types/ws": "^7.4.4",
    "axios": "^0.21.1",
    "canvas": "^2.7.0",
    "chalk": "^4.1.0",
    "copyfiles": "^2.4.1",
    "dayjs": "^1.10.4",
    "discord.js": "^12.5.1",
    "dotenv": "^9.0.2",
    "nodemon": "^2.0.7",
    "tslib": "^2.2.0",
    "typescript": "^4.1.3",
    "underscore": "^1.12.1"
  },
  "devDependencies": {
    "@types/jest": "^26.0.20",
    "@typescript-eslint/eslint-plugin": "^4.23.0",
    "@typescript-eslint/parser": "^4.23.0",
    "eslint": "^7.26.0",
    "eslint-config-prettier": "^8.3.0",
    "eslint-plugin-eslint-comments": "^3.2.0",
    "eslint-plugin-functional": "^3.2.1",
    "eslint-plugin-import": "^2.23.0",
    "jest": "^26.6.3",
    "prettier": "^2.3.0",
    "ts-jest": "^26.5.6",
    "ts-node-dev": "^1.1.1"
  },
  "prettier": {
    "singleQuote": true
  }
}

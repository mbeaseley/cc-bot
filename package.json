{
  "name": "cc-bot",
  "version": "2.0.0",
  "description": "A general discord bot with multiple functionalities",
  "main": "build/main.js",
  "scripts": {
    "build": "tsc && copyfiles -u 1 ./src/assets/images/*.png build",
    "postinstall": "npm run build",
    "prestart": "npm run build",
    "start": "node -r ts-node/register -r tsconfig-paths/register build/main.js",
    "dev": "nodemon",
    "lint": "eslint --ext ts .",
    "lint:fix": "npm run lint -- --fix",
    "npm:clear": "rm -rf $PWD/node_modules && rm $PWD/package-lock.json && npm cache verify && npm install"
  },
  "keywords": [],
  "author": "",
  "license": "ISC",
  "repository": {},
  "dependencies": {
    "@discordjs/builders": "^0.9.0",
    "@discordx/importer": "^1.1.2",
    "@discordx/utilities": "^3.1.5",
    "@types/underscore": "^1.11.4",
    "@types/urban-dictionary": "^3.0.0",
    "axios": "^0.24.0",
    "canvas": "^2.8.0",
    "chalk": "^4.1.1",
    "copyfiles": "^2.4.1",
    "dayjs": "^1.10.6",
    "discord.js": "^13.3.1",
    "discordx": "^9.0.1",
    "dotenv": "^10.0.0",
    "minecraft-server-util": "^4.1.2",
    "mongodb": "^4.2.0",
    "opusscript": "^0.0.8",
    "reflect-metadata": "^0.1.13",
    "rss-parser": "^3.12.0",
    "ts-node": "^10.4.0",
    "tsconfig-paths": "^3.12.0",
    "tslib": "^2.3.1",
    "underscore": "^1.13.1",
    "urban-dictionary": "^3.0.2"
  },
  "devDependencies": {
    "@types/node": "^16.11.10",
    "@types/ws": "^8.2.1",
    "@typescript-eslint/eslint-plugin": "^5.5.0",
    "@typescript-eslint/parser": "^5.5.0",
    "eslint": "^7.32.0",
    "eslint-config-prettier": "^8.3.0",
    "eslint-import-resolver-typescript": "^2.5.0",
    "eslint-plugin-import": "^2.25.3",
    "eslint-plugin-prettier": "^4.0.0",
    "nodemon": "^2.0.15",
    "prettier": "^2.5.0",
    "typescript": "^4.4.4"
  },
  "engines": {
    "node": "<=16.13.0",
    "npm": "<=8.1.0"
  },
  "nodemonConfig": {
    "ignore": [
      "**/*.test.ts",
      "**/*.spec.ts",
      ".git",
      "node_modules"
    ],
    "watch": [
      "src"
    ],
    "exec": "node -r tsconfig-paths/register -r ts-node/register ./src/main.ts",
    "ext": "ts, js"
  }
}
